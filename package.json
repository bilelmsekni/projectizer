{
  "name": "projectizer",
  "displayName": "Projectizer",
  "description": "Monorepo code manager",
  "version": "0.0.1",
  "publisher": "skible",
  "icon": "assets/projectizer_icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/mseknibilel/projectizer.git"
  },
  "bugs": {
    "url": "https://github.com/mseknibilel/projectizer/issues"
  },
  "keywords": [
    "angular monorepository"
  ],
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/tsconfig.json",
    "onCommand:extension.projectize"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.projectize",
        "title": "Projectize"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "tslint": "5.10.0",
    "typescript": "2.9.2",
    "vscode": "1.1.18"
  },
  "dependencies": {
    "fs-extra": "^7.0.0"
  }
}
